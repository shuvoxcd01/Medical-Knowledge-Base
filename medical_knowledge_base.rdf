<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE rdf:RDF [
<!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'> 
<!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
<!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
<!ENTITY base 'http://www.medical-knowledge-base.org/'>
<!ENTITY disease 'http://www.medical-knowledge-base.org/disease#'>
<!ENTITY symptom 'http://www.medical-knowledge-base.org/symptom#'>
<!ENTITY physician 'http://www.medical-knowledge-base.org/physician#'>
<!ENTITY property 'http://www.medical-knowledge-base.org/property#'>
<!ENTITY foaf 'http://xmlns.com/foaf/0.1/'>
]>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:base="http://www.medical-knowledge-base.org/"
    xmlns:disease="http://www.medical-knowledge-base.org/disease/"
    xmlns:property="http://www.medical-knowledge-base.org/property#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    >

    <!--Terminological Knowledge-->

    <!-- Properties -->
    <rdf:Property rdf:about="&property;icdCode">
        <rdfs:domain rdfs:nodeID="id1"></rdfs:domain>
        <rdfs:range rdfs:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
        <rdfs:label xml:lang="en">ICD Code</rdfs:label>
    </rdf:Property>

    <rdf:Property rdf:about="&property;icd11Code">
        <rdfs:subPropertyOf rdfs:resource="&property;icdCode"></rdfs:subPropertyOf>
        <rdfs:label xml:lang="en">ICD-11 Code</rdfs:label>
    </rdf:Property>

    <rdf:Property rdf:about="&property;hasSymptom">
        <rdfs:domain rdfs:resource="&disease;Disease"></rdfs:domain>
        <rdfs:range rdfs:resource="&symptom;Symptom"></rdfs:range>
        <rdfs:label xml:lang="en">has symptom</rdfs:label>
    </rdf:Property>

    <rdf:Property rdf:about="&property;treats">
        <rdfs:domain rdfs:resource="&physician;Physician"></rdfs:domain>
        <rdfs:range rdf:nodeID="id1"></rdfs:range>
        <rdfs:label xml:lang="en">treats</rdfs:label>
    </rdf:Property>

    <!-- Classes -->

    <!--Parent class of all diseases and symptoms-->
    <rdfs:Class rdf:nodeID="id1">
            <rdfs:label xml:lang="en">Disease Or Symptom</rdfs:label>
    </rdfs:Class>

    <!-- Diseases -->
    <rdfs:Class rdf:about="&disease;Disease">
        <rdfs:label xml:lang="en">Disease</rdfs:label>
        <rdfs:subClassOf rdf:nodeID="id1"></rdfs:subClassOf>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;InfectiousOrParasitic">
        <rdfs:label xml:lang="en">Infectious Or Parasitic Disease</rdfs:label>
        <rdfs:subClassOf rdf:resource="&disease;Disease"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/1435254666"></rdfs:seeAlso>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;Neoplasms">
        <rdfs:label xml:lang="en">Neoplasms</rdfs:label>
        <rdfs:subClassOf rdf:resource="&disease;Disease"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/1630407678"></rdfs:seeAlso>
        <rdfs:comment>An abnormal or uncontrolled cellular proliferation which is not coordinated with an 
        organism's requirements for normal tissue growth, replacement or repair.</rdfs:comment>
    </rdfs:Class>

    <rdfs:Class rdf:nodeID="id2">
        <rdfs:label xml:lang="en">Endocrine, nutritional or metabolic Diseases</rdfs:label>
        <rdfs:subClassOf rdf:resource="&disease;Disease"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/21500692"></rdfs:seeAlso>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;Endocrine">
        <rdfs:label xml:lang="en">Endocrine Diseases</rdfs:label>
        <rdfs:subClassOf rdf:nodeID="id2"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/461716838"></rdfs:seeAlso>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;Diabetes">
        <rdfs:label xml:lang="en">Diabetes Mellitus</rdfs:label>
        <rdfs:subClassOf rdf:resource="&disease;Endocrine"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/465177735"></rdfs:seeAlso>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;Nutritional">
        <rdfs:label xml:lang="en">Nutritional Disorders</rdfs:label>
        <rdfs:subClassOf rdf:nodeID="id2"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/1671987290"></rdfs:seeAlso>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;Metabolic">
        <rdfs:label xml:lang="en">Metabolic Disorders</rdfs:label>
        <rdfs:subClassOf rdf:nodeID="id2"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/155258022"></rdfs:seeAlso>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;Respiratory">
        <rdfs:label xml:lang="en">Diseases of the Respiratory System</rdfs:label>
        <rdfs:subClassOf rdf:resource="&disease;Disease"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/197934298"></rdfs:seeAlso>
    </rdfs:Class>

    <rdfs:Class rdf:about="&disease;LungInfection">
        <rdfs:label xml:lang="en">Lung infections</rdfs:label>
        <rdfs:subClassOf rdf:resource="&disease;Disease"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/915779102"></rdfs:seeAlso>
    </rdfs:Class>

    <!--Symptoms-->
    <rdfs:Class rdf:about="&symptom;Symptom">
        <rdfs:label xml:lang="en">Symptom</rdfs:label>
        <rdfs:subClassOf rdf:nodeID="id1"></rdfs:subClassOf>
    </rdfs:Class>

     <rdfs:Class rdf:about="&symptom;General">
        <rdfs:label xml:lang="en">General Symptoms</rdfs:label>
        <rdfs:subClassOf rdf:resource="&symptom;Symptom"></rdfs:subClassOf>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/1390117811"></rdfs:seeAlso>
    </rdfs:Class>

    <!--Physicians-->
    <rdfs:Class rdf:about="&physician;Physician">
        <rdf:label xml:lang="en">Physician</rdf:label>
        <rdf:type rdf:resource="&foaf;Person"></rdf:type>
    </rdfs:Class>

    <rdfs:Class rdf:about="&physician;Endocrinologist">
        <rdf:label xml:lang="en">Endocrinologist</rdf:label>
        <rdfs:subClassOf rdf:resource="&physician;Physician"></rdfs:subClassOf>
        <property:treats rdf:resource="&disease;Endocrine"></property:treats>
    </rdfs:Class>

    <rdfs:Class rdf:about="&physician;Gastroenterologist">
        <rdf:label xml:lang="en">Gastroenterologist</rdf:label>
        <rdfs:subClassOf rdf:resource="&physician;Physician"></rdfs:subClassOf>
    </rdfs:Class>

    <rdfs:Class rdf:about="&physician;Cardiologist">
        <rdf:label xml:lang="en">Cardiologist</rdf:label>
        <rdfs:subClassOf rdf:resource="&physician;Physician"></rdfs:subClassOf>
    </rdfs:Class>

    <rdfs:Class rdf:about="&physician;Hematologist">
        <rdf:label xml:lang="en">Hematologist</rdf:label>
        <rdfs:subClassOf rdf:resource="&physician;Physician"></rdfs:subClassOf>
    </rdfs:Class>

      <rdfs:Class rdf:about="&physician;Nephrologist">
        <rdf:label xml:lang="en">Nephrologist</rdf:label>
        <rdfs:subClassOf rdf:resource="&physician;Physician"></rdfs:subClassOf>
    </rdfs:Class>

      <rdfs:Class rdf:about="&physician;Neurologist">
        <rdf:label xml:lang="en">Neurologist</rdf:label>
        <rdfs:subClassOf rdf:resource="&physician;Physician"></rdfs:subClassOf>
    </rdfs:Class>

      <rdfs:Class rdf:about="&physician;Pulmonologist">
        <rdf:label xml:lang="en">Pulmonologist</rdf:label>
        <rdfs:subClassOf rdf:resource="&physician;Physician"></rdfs:subClassOf>
    </rdfs:Class>
    <!--Assertional Knowledge-->
    <rdf:Description rdf:about="&disease;Pneumonia">
        <property:icd11Code>CA40</property:icd11Code>
        <rdfs:label xml:lang="en">Pneumonia</rdfs:label>
        <rdfs:label xml:lang="bn">নিউমোনিয়া</rdfs:label>
        <rdf:type rdf:resource="&disease;LungInfection"></rdf:type>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/142052508"></rdfs:seeAlso>
        <property:hasSymptom rdf:resource="&symptom;Fever"></property:hasSymptom>
    </rdf:Description>

    <rdf:Description rdf:about="&disease;DiabetesType1">
        <property:icd11Code>5A10</property:icd11Code>
        <rdfs:label xml:lang="en">Type 1 Diabetes</rdfs:label>
        <rdfs:label xml:lang="bn">বহুমূত্ররোগ ধরণ ১</rdfs:label>
        <rdf:type rdf:resource="&disease;Diabetes"></rdf:type>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/1651053999"></rdfs:seeAlso>
    </rdf:Description>

    <rdf:Description rdf:about="&disease;DiabetesType2">
        <property:icd11Code>5A11</property:icd11Code>
        <rdfs:label xml:lang="en">Type 2 Diabetes</rdfs:label>
        <rdfs:label xml:lang="bn">বহুমূত্ররোগ ধরণ ২</rdfs:label>
        <rdf:type rdf:resource="&disease;Diabetes"></rdf:type>
        <rdfs:seeAlso rdf:resource="http://id.who.int/icd/entity/119724091"></rdfs:seeAlso>
    </rdf:Description>

    <rdf:Description rdf:about="&symptom;Fever">
        <property:icd11Code>MG26</property:icd11Code>
        <rdfs:label xml:lang="en">Fever of other or unknown origin</rdfs:label>
        <rdf:type rdf:resource="&symptom;General"></rdf:type>
        <rdf:seeAlso rdf:resource="http://id.who.int/icd/entity/915343154"></rdf:seeAlso>
    </rdf:Description>

    <rdf:Description rdf:about="&symptom;Chills">
        <property:icd11Code>MG21</property:icd11Code>
        <rdfs:label xml:lang="en">Chills</rdfs:label>
        <rdf:type rdf:resource="&symptom;General"></rdf:type>
        <rdf:seeAlso rdf:resource="http://id.who.int/icd/entity/1495646237"></rdf:seeAlso>
    </rdf:Description>

    <rdf:Description rdf:about="&symptom;Fatigue">
        <property:icd11Code>MG22</property:icd11Code>
        <rdfs:label xml:lang="en">Fatigue</rdfs:label>
        <rdf:type rdf:resource="&symptom;General"></rdf:type>
        <rdf:seeAlso rdf:resource="http://id.who.int/icd/entity/1109546957"></rdf:seeAlso>
    </rdf:Description>


</rdf:RDF>
